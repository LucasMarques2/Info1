#include <stdio.h>

//85428 Lucas Marques

int main(void) {
	
	int nota;
	
	printf("Ingrese su calificacion: ");
	scanf ("%d", &nota);
	
	if (nota>=90){
		printf("Su calificacion es: A ");
	}
	else{
		if(nota>=80 && nota<90){
		printf("Su calificacion es: B ");
		}
		else{
		if(nota>=70 && nota<80){
		printf("Su calificacion es: C ");
		}
	else{
		if(nota>=60 && nota<70){
		printf("Su calificacion es: D ");
		}
	else {
		printf("Su calificacion es: F ");
	}
	}
	}
	}
	return 0;
}
